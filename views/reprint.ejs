<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Reprint</title>

  <!-- Custom fonts for this template-->
  <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

  <!-- Custom styles for this template-->
  <link href="css/sb-admin-2.min.css" rel="stylesheet">

  <!-- Custom styles for this page -->
  <link href="vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">

  <link href="vendor/toastr/toastr.min.css" rel="stylesheet">

</head>

<body id="page-top" class="sidebar-toggled">

  <!-- Page Wrapper -->
  <div id="wrapper">

    <!-- Sidebar -->
    <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion toggled" id="accordionSidebar">

      <!-- Sidebar - Brand -->
      <a class="sidebar-brand d-flex align-items-center justify-content-center" href="index.html">
        <div class="sidebar-brand-icon rotate-n-15">
          <i class="fas fa-laugh-wink"></i>
        </div>
        <div class="sidebar-brand-text mx-3">rest<sup>ZPL</sup></div>
      </a>

      <!-- Divider -->
      <hr class="sidebar-divider my-0">

      <li class="nav-item">
        <a class="nav-link" href="/">
          <i class="fas fa-fw fa-tachometer-alt"></i>
          <span>Overview</span></a>
      </li>
      <li class="nav-item active">
        <a class="nav-link" href="/reprint">
          <i class="fas fa-fw fa-redo"></i>
          <span>Reprint</span></a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/label">
          <i class="fas fa-fw fa-tags"></i>
          <span>Label</span></a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/printer">
          <i class="fas fa-fw fa-print"></i>
          <span>Printer</span></a>
      </li>

      <!-- Divider -->
      <hr class="sidebar-divider d-none d-md-block">

      <!-- Sidebar Toggler (Sidebar) -->
      <div class="text-center d-none d-md-inline">
        <button class="rounded-circle border-0" id="sidebarToggle"></button>
      </div>

    </ul>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content" style="padding-top: 30px;">

        <!-- Begin Page Content -->
        <div class="container-fluid">

          <!-- Page Heading -->
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <h1 class="h3 mb-0 text-gray-800">reprint</h1>
            <div class="d-none d-sm-inline-block">
              <div class="dropdown shadow-sm">
                <button class="btn btn-sm btn-primary dropdown-toggle" id="timespan" data-time="<%= timespan %>" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  last 24 hours
                </button>
                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                  <a class="dropdown-item time-item" data-time="24" href="#">last 24 hours</a>
                  <a class="dropdown-item time-item" data-time="168" href="#">last 7 days</a>
                  <a class="dropdown-item time-item" data-time="720" href="#">last 30 days</a>
                </div>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-lg-12">
              <!-- Basic Card Example -->
              <div class="card shadow mb-4">
                <div class="card-header py-3">
                  <h6 class="m-0 font-weight-bold text-primary">Job Overview</h6>
                </div>
                <div class="card-body">
                  <div class="table-responsive">
                    <table class="table table-bordered" id="table" width="100%" cellspacing="0">
                      <thead>
                        <tr>
                          <th>Printer</th>
                          <th>Label</th>
                          <th>Date</th>
                          <th>Error</th>
                          <th>Action</th>
                        </tr>
                      </thead>
                      <tbody>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>

        </div>
        <!-- /.container-fluid -->

      </div>
      <!-- End of Main Content -->

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  <!-- Modal -->
  <div class="modal fade" id="jobModal" tabindex="-1" role="dialog" aria-labelledby="jobModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="jobModalLabel">Job</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <ul class="nav nav-pills" id="myTab" role="tablist">
            <li class="nav-item">
              <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Reprint</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Data & ZPL</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">JSON</a>
            </li>
          </ul>
          <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
              <div class="container-fluid" style="margin-top: 20px;">
                <div class="row">
                  <div class="col-lg-12 col-md-12 ">
                    <h6 class="m-0 font-weight-bold text-primary">Preview</h6>
                    <div id="imgContainer">
                      <img class="img-fluid shadow w-75" />
                    </div>
                  </div>
                  <div class="col-lg-12 col-md-12" style="margin-top: 20px;">
                    <div class="form-group">
                      <h6 class="m-0 font-weight-bold text-primary">generated ZPL code</h6>
                      <!-- <input type="text" class="form-control" id="newzpl" placeholder="New ZPL"> --->
                      <textarea class="form-control" id="gzpl" placeholder="generated data" rows="10"></textarea>
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-6 ">
                    <div class="dropdown">
                      <button class="btn btn-secondary dropdown-toggle" type="button" id="jobprinter" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Printer
                      </button>
                      <div class="dropdown-menu printer-dropdown" aria-labelledby="dropdownMenuButton">
                      </div>
                    </div>
                  </div>
                  <div class="col-6 ">
                    <button class="btn btn-primary" id="reprint"> reprint</button>
                  </div>
                </div>
              </div>
            </div>
            <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
              <div class="container-fluid">
                <div class="row" style="margin-top:20px;">
                  <div class="col-lg-12 col-md-12 ">
                    <div class="form-group">
                      <h6 class="m-0 font-weight-bold text-primary">Data send with Job</h6>
                      <!-- <input type="text" class="form-control" id="newzpl" placeholder="New ZPL"> --->
                      <textarea class="form-control" id="data" placeholder="data" rows="6" readonly></textarea>
                    </div>
                  </div>
                  <div class="col-lg-12 col-md-12 ">
                    <div class="form-group">
                      <h6 class="m-0 font-weight-bold text-primary">ZPL code saved in Label</h6>
                      <!-- <input type="text" class="form-control" id="newzpl" placeholder="New ZPL"> --->
                      <textarea class="form-control" id="zpl" placeholder="ZPL" rows="6" readonly></textarea>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">
              <div class="container-fluid">
                <div class="row" style="margin-top:20px;">
                  <div class="col-lg-12 col-md-12 ">
                    <div class="form-group">
                      <h6 class="m-0 font-weight-bold text-primary">job as JSON</h6>
                      <!-- <input type="text" class="form-control" id="newzpl" placeholder="New ZPL"> --->
                      <textarea class="form-control" id="json" placeholder="job information as json" rows="15" readonly></textarea>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>

