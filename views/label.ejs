            <div class="col-lg-6">
              <!-- Basic Card Example -->
              <div class="card shadow mb-4">
                <div class="card-header py-3">
                  <h6 class="m-0 font-weight-bold text-primary">Create Label</h6>
                </div>
                <div class="card-body">
                  <form>
                    <div class="form-group">
                      <label for="newname">Name</label>
                      <input type="text" class="form-control" id="newname" placeholder="Label Name">
                    </div>
                    <div class="form-row">
                      <div class="form-group col-md-6 col-lg-6">
                        <label for="newwidth">Width</label>
                        <input type="number" class="form-control" id="newwidth" placeholder="Width in inches">
                      </div>
                      <div class="form-group col-md-6 col-lg-6">
                        <label for="newheight">Height</label>
                        <input type="number" class="form-control" id="newheight" placeholder="Height in inches">
                      </div>
                    </div>
                    <div class="btn btn-primary" id="create">Create</div>
                  </form>
                </div>
              </div>
            </div>
            <div class="col-lg-6">
              <!-- Basic Card Example -->
              <div class="card shadow mb-4">
                <div class="card-header py-3">
                  <h6 class="m-0 font-weight-bold text-primary">Delete Label</h6>
                </div>
                <div class="card-body">
                  <div class="dropdown">
                    <button class="btn btn-secondary dropdown-toggle" id="delete-label" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                      Label
                    </button>
                    <div class="dropdown-menu label-dropdown" aria-labelledby="dropdownMenuButton">
                    </div>
                  </div>
                </div>
              </div>
            </div>
